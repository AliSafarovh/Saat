@model CreateGenderDto
@{
    ViewData["Title"] = "CreateGender";
    Layout = "~/Views/AdminLayout/Index.cshtml";
}
<div class="content">
    <div class="container-fluid">
        <h4 class="page-title">Gender əməliyyatları</h4>
        <div class="row">
            <div class="col-md-12">
                <form method="post">
                    <div class="card">
                        <div class="card-header">
                            <div class="card-title">Yeni Gender Girişi</div>
                        </div>
                        <div class="card-body">
                            <div class="form-group">
                                <label for="gendername">Gender Adı</label>
                                <input type="text" class="form-control" asp-for="Name" id="gendername" required>
                            </div>
                        </div>
                        <div class="card-action">
                            <button class="btn btn-success">Təsdiq Et</button>
                            <a href="/Gender/CreateGender/" class="btn btn-secondary">İmtina Et</a>
                        </div>
                    </div>
                </form>
            </div>
        </div>
    </div>
</div>



